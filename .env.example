# Medical Knowledge Platform Environment Variables

# Application Settings
APP_NAME=Medical Knowledge Platform
VERSION=3.0.0
ENVIRONMENT=development
DEBUG=true

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost/medical_platform

# AI Service Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Additional AI providers
# CLAUDE_API_KEY=your_claude_api_key_here
# GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Research APIs
# PUBMED_API_KEY=your_pubmed_api_key_here

# Security
SECRET_KEY=your_secret_key_here

# Medical Domain Settings
MEDICAL_SPECIALTIES=neurosurgery,cardiology,oncology
ENABLE_CONTEXTUAL_AI=true
QUERY_ENHANCEMENT_LEVEL=3